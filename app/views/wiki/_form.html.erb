<!-- form_for(@article, url: 'wiki_index_path', class: "form-group") -->

<%= form_for(@article, url: { controller: 'wiki' }, class: "form-group") do |f| %>
  <%= f.label 'Title', class: 'control-label' %>
  <%= f.text_field :title, class: 'form-control' %>
  <%= f.label 'Content', class: 'control-label' %>
  <%= f.text_field :content, class: 'form-control' %>
  <%= f.submit class: 'btn btn-primary', style: 'margin: 50px' %>
<% end %>